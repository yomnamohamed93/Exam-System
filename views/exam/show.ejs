<div class="container">
    <div class="row">
        <div class="col-xs-8 col-sm-8 push-sm-2 push-xs-2 text-center">
            <br>
            <div>
                <h2>Adminstration Panel</h2>
                <h3>Question Details</h3>
            </div>
            <table class="table table-striped table-bordered text-center">
                <thead>
                    <tr>
                        <th class="text-center">
                            <p> CH:
                            </p>
                        </th>
                    </tr>
                </thead>
                <tbody>
                    <tr>
                        <td>
                            <p>
                                Difficulty:
                            </p>

                        </td>
                    </tr>
                    <tr>
                        <td>
                            <div class="float-center">
                                <% _.each(exam, function(question, i) { %>
                                    <p>
                                        Answer
                                        <%=i+1%>:
                                            <%= question.question_head %>
                                    </p>
                                    <% }); %>
                            </div>
                        </td>
                    </tr>
                    <tr>
                        <td class="text-center">
                            <p>
                                Correct answer:
                            </p>
                        </td>
                    </tr>
                </tbody>
            </table>
        </div>
    </div>
</div>